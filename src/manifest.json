{
    "manifest_version": 2,
    "applications": {
        "gecko": {
            "id": "filelink@johannes-endres.de",
            "strict_min_version": "68.0"
        }
    },
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "homepage_url": "https://gitlab.com/joendres/filelink-nextcloud",
    "author": "Johannes Endres",
    "version": "3.0.0",
    "icons": {
        "48": "icon48.png"
    },
    "background": {
        "scripts": [
            "cloudconnection.js",
            "background.js"
        ]
    },
    "cloud_file": {
        "name": "*cloud",
        "management_url": "management.html",
        "data_format": "ArrayBuffer"
    },
    "permissions": [
        "<all_urls>",
        "storage"
    ],
    "default_locale": "en"
}